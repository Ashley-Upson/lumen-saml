{
    "name": "Ashley-Upson/lumen-saml",
    "description": "A Lumen 5.4 / 5.5 SAML plugin that transforms laravel into an IDP - a fork from kingstarter/laravel/saml.",
    "keywords": ["lumen", "saml", "illuminate", "auth", "acl", "login", "sso", "idp", "sp"],
    "license": "MIT",
    "version": "dev-master",
    "authors": [
        {
            "name": "Ashley Upson",
            "email": "ashley@aupson.co.uk"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.2",
        "lightsaml/lightsaml": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "AshleyUpson\\LumenSaml\\": "src"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "AshleyUpson\\LumenSaml\\LaravelSamlServiceProvider"
          ]
        }
    }
}
